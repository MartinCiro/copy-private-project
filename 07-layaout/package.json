{
  "name": "inventario-backend",
  "version": "1.0.0",
  "description": "Backend para control de in/egresos personalizado",
  "main": "./src/index.js",
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "start": "concurrently \"nodemon ./src/index.js\" \"live-server front\"",
    "build": "powershell -Command \"if (-not (Test-Path dist)) { New-Item -ItemType Directory -Path dist }; Get-ChildItem -Path src -Recurse -Filter '*.js' | ForEach-Object { Get-Content $_.FullName } | Out-File -FilePath 'dist/bundle.js' -Force\""
  },
  "keywords": [
    "control",
    "ingresos",
    "egresos"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrency": "^0.1.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "live-server": "^1.2.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.5",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}
